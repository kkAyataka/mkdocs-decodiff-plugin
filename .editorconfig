root = true

[*]
insert_final_newline = true
trim_trailing_whitespace = true

[*.md]
charset = utf-8
end_of_line = lf
indent_style = space
indent_size = 4
trim_trailing_whitespace = false

[*.py]
charset = utf-8
indent_style = space
indent_size = 4

[*.json]
charset = utf-8
indent_style = space
indent_size = 2
